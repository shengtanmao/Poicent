int *int_p()
{
    int *a;
    return a;
}

int **int_pp()
{
    int **a;
    return a;
}

float *float_p()
{
    float *a;
    return a;
}

float **float_pp()
{
    float **a;
    return a;
}

bool *bool_p()
{
    bool *a;
    return a;
}

bool **bool_pp()
{
    bool **a;
    return a;
}

int main()
{
    int *ip;
    int **ipp;
    float *fp;
    float **fpp;
    bool *bp;
    bool **bpp;

    ip = int_p();
    ipp = int_pp();
    fp = float_p();
    fpp = float_pp();
    bp = bool_p();
    bpp = bool_pp();

    return 0;
}